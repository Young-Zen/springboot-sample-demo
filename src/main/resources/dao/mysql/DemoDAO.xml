<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.springbootsample.demo.dao.mysql.DemoDAO">
    <update id="updateAge">
        UPDATE t_boot_demo
        SET age = age + 1
        WHERE pk_demo_id = #{id}
    </update>
</mapper>